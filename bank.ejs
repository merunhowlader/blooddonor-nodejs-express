<html>
	<head>
		<title>Blood bank View | Page</title>
		
		<script  src="jquery-3.3.1.js"></script>
		<script  src="test.js"></script>
	</head>
	<body>
		
		<div style="background-color:green;" size="35">
			<table align="center" width="150%">
				<tr>
					<td><a href="/admin">Back</a><button><a href="/admin">HOME  </a></button></td>
					<td><button><a href="/logout">Log Out</a></button></td>
			
				</tr>
				<tr>

		     			<h1>Bank Details</h1>
				     
				    </tr>


			</table>
			<table border="1" align="center" style="background-color: skyblue">
				<tr>
					<th>ID</th>
					<th>Blood Bank Name</th>
					<th>Email</th>
					<th>Contact</th>
					<th>Division</th>
					<th>Area</th>
					<th>Operatios</th>
				</tr>
				<% for(var i=0;i<list.length;i++){%>

					<tr>
						<td><%= list[i].id %></td>
						<td><%= list[i].gname %></td>
						<td><%= list[i].email %></td>
						<td><%= list[i].phn %></td>
						<td><%= list[i].divi %></td>
						<td><%= list[i].area %></td>
						<td><a href="/admin/edit_bank/<%= list[i].id %>">Edit </a> | <a href="/admin/delete_bank/<%= list[i].id %>">Delete </a></td>
					</tr>



				<% } %>
			</table>

		</div>
		


		
		
	</body>
</html>